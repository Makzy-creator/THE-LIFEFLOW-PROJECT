{
  "name": "innovation-blood-donation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "compile": "npx hardhat compile",
    "deploy:mumbai": "npx hardhat run scripts/deploy.js --network mumbai",
    "deploy:polygon": "npx hardhat run scripts/deploy.js --network polygon",
    "deploy:local": "npx hardhat run scripts/deploy.js --network hardhat",
    "test:contracts": "npx hardhat test",
    "verify:mumbai": "npx hardhat verify --network mumbai",
    "verify:polygon": "npx hardhat verify --network polygon"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@perawallet/connect": "^1.4.2",
    "@randlabs/myalgo-connect": "^1.4.2",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.87.6",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.81.5",
    "@tensorflow/tfjs": "^4.13.0",
    "@web3modal/ethereum": "^2.7.1",
    "@web3modal/react": "^2.7.1",
    "@web3modal/wagmi": "^5.1.11",
    "date-fns": "^2.30.0",
    "ethers": "^6.8.0",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-player": "^2.13.0",
    "react-router-dom": "^6.15.0",
    "react-webcam": "^7.1.1",
    "remark-gfm": "^4.0.0",
    "socket.io-client": "^4.7.2",
    "viem": "^2.31.4",
    "wagmi": "^2.15.6"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "hardhat": "^2.17.2",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^7.0.0"
  }
